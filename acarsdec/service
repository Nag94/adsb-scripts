[Unit]
Wants=network.target
After=network.target

[Service]
User=acarsdec
EnvironmentFile=/etc/default/acarsdec
SyslogIdentifier=acarsdec

ExecStart=/usr/local/bin/acarsdec \
$OPTIONS0 \
$OPTIONS1 \
$OPTIONS2 \
$OPTIONS3 \
$OPTIONS4 \
$OPTIONS5 \
$OPTIONS6 \
$OPTIONS7 \
$OPTIONS8 \
$OPTIONS9

Type=simple
Restart=always
RestartSec=30

[Install]
WantedBy=default.target
